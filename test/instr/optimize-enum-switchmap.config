{
  "redex" : {
    "passes" : [
      "RemoveUnreachablePass",
      "ResolveRefsPass",
      "DedupBlocksPass",
      "OptimizeEnumsPass",
      "LocalDcePass",
      "RemoveUnreachablePass",
      "RegAllocPass"
    ]
  },
  "ResolveRefsPass" : {
    "excluded_externals" : [
      "Landroid/os/BaseBundle",
      "Ljava/lang/reflect/Executable;"
