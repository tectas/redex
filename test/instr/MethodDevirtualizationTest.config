{
  "redex" : {
    "passes" : [
      "MethodDevirtualizationPass"
    ]
  },
  "no_devirtualize_annos" : [                                                 
     "Lcom/facebook/annotations/DoNotDevirtualize;"                         
  ],
  "MethodDevirtualizationPass": {
    "staticize_vmethods_not_using_this" : true,
    "staticize_dmethods_not_using_this" : true,
    "staticize_vmethods_using_this" : true,
    "staticize_dmethods_using_this" : true
  }
}
